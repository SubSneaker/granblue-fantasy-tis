{
    "compilerOptions": {
      "outDir": "./dist",
      "noImplicitAny": true,
      "module": "ES2020",
      "target": "ES6",
      "allowJs": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true
      
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"],
    "typeRoots": ["node_modules/@types", "src/types"]
  }