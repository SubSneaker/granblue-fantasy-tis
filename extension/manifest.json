{
    "manifest_version": 3,
    "name": "Granblue Fantasy: Resubbed",
    "version": "0.0.1",
    "description": "Description of your extension",
    "permissions": ["activeTab", "cookies", "scripting", "webRequest", "storage"],
    // "background": {
    //   "service_worker": "background.js"
    // },
      "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://game.granbluefantasy.jp/*",
            "https://game.granbluefantasy.jp/#quest/scene/*"
            // "https://developer.chrome.com/docs/extensions/*",
            // "https://developer.chrome.com/docs/webstore/*"
          ]
        }
      ],
      "web_accessible_resources": [{
        "matches": ["https://game.granbluefantasy.jp/*"],
        "resources": ["modules/*.js"]
      }],
    "action": {
      "default_icon": "GBFR_icon.png",
      "default_popup": "popup.html"
    }
    // "icons": {
    //   "16": "icon16.png",
    //   "48": "icon48.png",
    //   "128": "icon128.png"
    // }
  }